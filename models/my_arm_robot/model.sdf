<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="my_arm_robot">

<static>0</static>

 <link name='chassis'>
            <pose>0 0 .1 0 0 0</pose>

            <collision name='collision'>
              <geometry>
                <box>
                  <size>.4 .2 .1</size>
                </box>
              </geometry>
            </collision>

            <visual name='visual'>
              <geometry>
                <box>
                  <size>.4 .2 .1</size>
                </box>
              </geometry>
            </visual>

 <collision name='caster_collision'>
            <pose>-0.15 0 -0.05 0 0 0</pose>
            <geometry>
                <sphere>
                <radius>.05</radius>
              </sphere>
            </geometry>

            <surface>
              <friction>
                <ode>
                  <mu>0</mu>
                  <mu2>0</mu2>
                  <slip1>1.0</slip1>
                  <slip2>1.0</slip2>
                </ode>
              </friction>
            </surface>
          </collision>

          <visual name='caster_visual'>
            <pose>-0.15 0 -0.05 0 0 0</pose>
            <geometry>
              <sphere>
                <radius>.05</radius>
              </sphere>
            </geometry>
          </visual>

          </link>

<link name='arm1'>
 	 <pose>0 0 .175 0 0 0</pose>
	 <collision name='collision'>		
	      <geometry>
	        <box>
	          <size>.1 .1 .05</size>
	        </box>
	      </geometry>
	    </collision>

	    <visual name='visual'>
	      <geometry>
	        <box>
	          <size>.1 .1 .05</size>
	        </box>
	      </geometry>
	    </visual>
</link>

<link name='arm2'>
 	 <pose>.0 0 .4 0 0 0</pose>
	 <collision name='collision'>		
	      <geometry>
	        <box>
	          <size>.05 .05 .4</size>
	        </box>
	      </geometry>
	    </collision>

	    <visual name='visual'>
	      <geometry>
	        <box>
	          <size>.05 .05 .4</size>
	        </box>
	      </geometry>
	    </visual>
</link>

<link name='hand1'>
 	 <pose>0 .1 .6 0 0 0</pose>
	 <collision name='collision'>		
	      <geometry>
	        <box>
	          <size>.05 .1 .05</size>
	        </box>
	      </geometry>
	    </collision>

	    <visual name='visual'>
	      <geometry>
	        <box>
	          <size>.05 .1 .05</size>
	        </box>
	      </geometry>
	    </visual>
</link>

<link name='hand2'>
 	 <pose>0 -.1 .6 0 0 0</pose>
	 <collision name='collision'>		
	      <geometry>
	        <box>
	          <size>.05 .1 .05</size>
	        </box>
	      </geometry>
	    </collision>

	    <visual name='visual'>
	      <geometry>
	        <box>
	          <size>.05 .1 .05</size>
	        </box>
	      </geometry>
	    </visual>
</link>

<link name='hand1_2'>
 	 <pose>0 .25 .6 0 0 0</pose>
	 <collision name='collision'>		
	      <geometry>
	        <box>
	          <size>.1 .15 .025</size>
	        </box>
	      </geometry>
	    </collision>

	    <visual name='visual'>
	      <geometry>
	        <box>
	          <size>.1 .15 .025</size>
	        </box>
	      </geometry>
	    </visual>
</link>

<link name='hand2_2'>
 	 <pose>0 -.25 .6 0 0 0</pose>
	 <collision name='collision'>		
	      <geometry>
	        <box>
	          <size>.1 .15 .025</size>
	        </box>
	      </geometry>
	    </collision>

	    <visual name='visual'>
	      <geometry>
	        <box>
	          <size>.1 .15 .025</size>
	        </box>
	      </geometry>
	    </visual>
</link>


















<link name="left_wheel">
        <pose>0.1 0.13 0.1 0 1.5707 1.5707</pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

<link name="right_wheel">
        <pose>0.1 -0.13 0.1 0 1.5707 1.5707</pose>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.05</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

   <joint type="revolute" name="left_wheel_hinge">
        <pose>0 0 -0.03 0 0 0</pose>
        <child>left_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
      </joint>

      <joint type="revolute" name="right_wheel_hinge">
        <pose>0 0 0.03 0 0 0</pose>
        <child>right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
      </joint>


 <joint type="revolute" name="shoulder">
        <pose>0 0 0.03 0 0 0</pose>
        <child>right_wheel</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 1 0</xyz>
        </axis>
      </joint>



      <joint type="revolute" name="arm1_hinge">
	<pose>0 0 .15 0 0 0</pose>
	<child>arm1</child>
	<parent>chassis</parent>
	<axis>
	  <xyz>0 0 1</xyz>
	</axis>
      </joint>

      <joint type="revolute" name="arm2_hinge">
	<pose>0 0 -.2 0 0 0</pose>
	<child>arm2</child>
	<parent>arm1</parent>
	<axis>
	  <xyz>0 1 0</xyz>
	</axis>
      </joint>


      <joint type="revolute" name="hand1_hinge">
	<pose>.03 0 0 0 0 0</pose>
	<child>hand1</child>
	<parent>arm2</parent>
	<axis>
	  <xyz>1 0 0</xyz>
	</axis>
      </joint>

      <joint type="revolute" name="hand2_hinge">
	<pose>.03 0 0 0 0 0</pose>
	<child>hand2</child>
	<parent>arm2</parent>
	<axis>
	  <xyz>1 0 0</xyz>
	</axis>
      </joint>

      <joint type="revolute" name="hand1_2_hinge">
	<pose>.03 0 0 0 0 0</pose>
	<child>hand1_2</child>
	<parent>hand1</parent>
	<axis>
	  <xyz>1 0 0</xyz>
	</axis>
      </joint>

      <joint type="revolute" name="hand2_2_hinge">
	<pose>.03 0 0 0 0 0</pose>
	<child>hand2_2</child>
	<parent>hand2</parent>
	<axis>
	  <xyz>1 0 0</xyz>
	</axis>
      </joint>

    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <robotNamespace>my_arm_robot</robotNamespace>
      <alwaysOn>true</alwaysOn>
      <updateRate>100.0</updateRate>
      <leftJoint>left_wheel_hinge</leftJoint>
      <rightJoint>right_wheel_hinge</rightJoint>
      <wheelSeparation>0.4</wheelSeparation>
      <wheelDiameter>0.215</wheelDiameter>
<!-- Following 2lines were came from gazebosim gazebo_ros topic page.
      <wheelSeparation>0.5380</wheelSeparation>
      <wheelDiameter>0.2410</wheelDiameter>
-->
      <robotBaseFrame>chassis</robotBaseFrame>
      <torque>200</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <broadcastTF>true</broadcastTF>
    </plugin>
      
    <plugin name="arm_controller" filename="libgazebo_ros_my_arm.so">
      <robotNamespace>my_arm_robot</robotNamespace>
      <alwaysOn>true</alwaysOn>
      <updateRate>100.0</updateRate>

      <shoulderJoint>shoulder</shoulderJoint>
      <elbowJoint>arm1_hinge</elbowJoint>
      <wristJoint>arm2_hinge</wristJoint>
      <finger1Joint>hand1_hinge</finger1Joint>
      <finger2Joint>hand2_hinge</finger2Joint>
      <finger12Joint>hand1_2_hinge</finger12Joint>
      <finger22Joint>hand2_2_hinge</finger22Joint>

<!-- Following 2lines were came from gazebosim gazebo_ros topic page.
      <wheelSeparation>0.4</wheelSeparation>
      <wheelDiameter>0.215</wheelDiameter>
      <wheelSeparation>0.5380</wheelSeparation>
      <wheelDiameter>0.2410</wheelDiameter>
      <robotBaseFrame>chassis</robotBaseFrame>
      <torque>200</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <broadcastTF>true</broadcastTF>
-->
    </plugin>
      
  </model>
</sdf>
