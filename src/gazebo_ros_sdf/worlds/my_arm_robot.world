<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <!-- A robot : my_robot -->
    <include>
      <pose>1 1 0.5 0 0 0</pose>
      <uri>model://my_arm_robot</uri>
    </include>
  </world>
</sdf>
